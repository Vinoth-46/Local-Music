{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"baseUrl": ".",
		"paths": {
			"@/components/*": ["src/components/*"],
			"@/screens/*": ["src/screens/*"],
			"@/player/*": ["src/player/*"],
			"@/state/*": ["src/state/*"],
			"@/utils/*": ["src/utils/*"]
		},
		"types": ["react", "react-native"]
	}
}